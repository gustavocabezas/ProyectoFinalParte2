    <script type="text/javascript">
        function buscarPeliculas() {
            var keyword = document.getElementById("keysss").value;
            // Realiza la búsqueda de películas aquí o redirige a la página de resultados.
            window.location.href = "/PeliculasPorNombre.aspx?keyword=" + keyword;
        }

        // Cierre de sesión
        var tiempoInactividad = 0;

        function resetearTiempoDeInactividad() {
            tiempoInactividad = 0;
        }

        function incrementarTiempoInactividad() {
            tiempoInactividad++;
            //console.log(tiempoInactividad);
            if (tiempoInactividad >= 5) { // 5 minutos
                window.location.href = '/Paginas/Logout.aspx'; // Página de
            }
        }

        // Eventos que resetean el contador de inactividad
        window.onload = resetearTiempoDeInactividad;
        window.onmousemove = resetearTiempoDeInactividad;
        window.onmousedown = resetearTiempoDeInactividad; // clicks, etc.
        window.ontouchstart = resetearTiempoDeInactividad;
        window.onclick = resetearTiempoDeInactividad;
        window.onkeypress = resetearTiempoDeInactividad;
        window.addEventListener('scroll', resetearTiempoDeInactividad, true);

        // Comprobar cada minuto si el usuario ha estado inactivo
        window.setInterval(incrementarTiempoInactividad, 60000); // 60 segundos
    </script>
